{
  "name": "r-term-dict",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:kkweon/r-term-dict.git",
  "author": "Kyung Mo Kweon <kkweon@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon",
    "clean": "rm -rf build",
    "cleaner": "rm -rf node_modules && yarn clean",
    "test": "NODE_ENV=test mocha -r ts-node/register tests/**/*.ts",
    "docker:dev": "NODE_ENV=development yarn docker:clean && docker-compose -f docker-compose.yml up",
    "docker:test": "NODE_ENV=test yarn docker:clean && docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "docker:clean": "docker-compose down -v",
    "docker:prod": "NODE_ENV=production yarn docker:clean && docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "copy": "yarn copy:data && yarn copy:public",
    "copy:data": "mkdir -p build/data && cp data/words.json build/data/",
    "copy:public": "mkdir -p build/src/server/public && cp src/server/public/* build/src/server/public/",
    "seed": "yarn ts-node bin/run_seeds.ts",
    "build": "NODE_ENV=production yarn build:server && yarn build:front && yarn copy",
    "build:server": "yarn tsc && yarn copy:data",
    "build:front": "yarn webpack --optimize-minimize"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/compression": "^0.0.35",
    "@types/cors": "^2.8.3",
    "@types/express": "^4.11.1",
    "@types/helmet": "^0.0.37",
    "@types/mocha": "^2.2.48",
    "@types/mongoose": "^5.0.2",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/redux-form": "^7.0.14",
    "@types/redux-logger": "^3.0.5",
    "@types/redux-promise-middleware": "^0.0.9",
    "@types/webpack": "^3.8.8",
    "awesome-typescript-loader": "^3.4.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "emotion": "^9.0.2",
    "emotion-theming": "^9.0.0",
    "express": "^4.16.2",
    "helmet": "^3.11.0",
    "http-status-codes": "^1.3.0",
    "mongoose": "^5.0.6",
    "morgan": "^1.9.0",
    "nodemon": "^1.15.1",
    "pug": "^2.0.0-rc.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-emotion": "^9.0.2",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.0.0",
    "ts-node": "^5.0.0",
    "typescript": "^2.7.2",
    "webpack": "^3.11.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "mocha": "^5.0.1",
    "supertest": "^3.0.0",
    "tslint": "^5.9.1"
  }
}
